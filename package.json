{
  "name": "chkutils",
  "version": "1.0.2",
  "sideEffects": false,
  "type": "module",
  "module": "./dist/chkutils.js",
  "main": "./dist/chkutils.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/chkutils.js",
      "require": "./dist/chkutils.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build-l": "vite build",
    "build-h": "tsc && vite build",
    "preview": "vite preview",
