{
  "name": "chkutils",
  "version": "1.0.5",
  "sideEffects": false,
  "type": "module",
  "module": "./dist/chkutils.esm.js",
  "main": "./dist/chkutils.common.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "node": "./dist/chkutils.common.js",
        "default": "./dist/chkutils.esm.js"
      },
      "types": "./dist/types/index.d.ts",
      "require": "./dist/chkutils.common.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "ts-check": "tsc",
    "build-l": "vite build",
    "build-h": "tsc && vite build",
    "preview": "vite preview",
    "generate-types": "tsc --declaration --emitDeclarationOnly --outDir ./dist",
    "generate-md-docs_all-types": "npm run build-to-deploy && npx api-extractor run",
    "build-to-deploy": "npm run build-l && npm run generate-types",
    "deploy-to-npm": "vite build && npm publish",
    "test-ui": "cypress open",
    "test-unit-all": "npx cypress run --component"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.50.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "cypress": "^14.0.3",
    "typescript": "~5.7.2",
    "vite": "^6.1.0"
  },
  "author": "Segey Zhuravlev",
  "license": "MIT",
  "description": "package for type checking and various utilities",
  "keywords": [
    "checkers",
    "utils",
    "helpers",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cossack-don/chkutils.git"
  }
}
