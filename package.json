    "deploy-to-npm": "vite build && npm publish",
    "test-ui": "cypress open",
    "test-unit-all": "npx cypress run --component"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "cypress": "^14.0.3",
    "typescript": "~5.7.2",
    "vite": "^6.1.0"
  },
  "author": "Segey Zhuravlev",
  "license": "MIT",
  "description": "package for type checking and various utilities",
  "keywords": [
    "checkers",
    "utils",
    "helpers",
    "modules"
